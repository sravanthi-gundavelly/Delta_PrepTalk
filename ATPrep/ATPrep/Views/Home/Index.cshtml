<script>

        var input = document.getElementById("UserName");
        input.addEventListener("keyup", function (event) {
            if (event.keyCode === 13) {
                event.preventDefault();
                document.getElementById("loginBtn").click();
            }
        });

        var pas = document.getElementById("password");
        pas.addEventListener("keyup", function (event) {
            if (event.keyCode === 13) {
                event.preventDefault();
                document.getElementById("loginBtn").click();
            }
        });


    function login() {
        localStorage.setItem("UserName","");
        localStorage.setItem("UserID", "");
        localStorage.setItem("session", "");

            var data = new FormData();
            var UserName = $("#UserName").val();
            var Password = $("#password").val();

            data.append('userid', UserName);
            data.append('password', Password);
            $.ajax({
              url: "https://preptalk.bfmdev1.com/validate_user",
                 // url: "http://p0152610:81/validate_user",
                type: 'POST',
                data: data,
                processData: false,
                contentType: false,
                success: function (res) {
                    console.log(res);
                    var User_ID = res.id;
                    localStorage.setItem("UserID", User_ID);
                    var username = res.name.split(" ");
                    localStorage.setItem("UserName", username[0]);
                    localStorage.setItem("userimage", res.userimage);
                    var url = '@Url.Action("Home", "Home")';
                    var url1 = '@Url.Action("Home", "Index")';
                    if (User_ID != null) {
                        window.location.href = url;
                    }
                    else {
                        window.location.href = url1;
                    }
                },
                error: function (xhr, status, error) {
                  //  alert("error");
                }
            });
        }




</script>


